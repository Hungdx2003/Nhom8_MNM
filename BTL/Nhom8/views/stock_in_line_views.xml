<odoo>
    <record id="view_stock_in_line_form" model="ir.ui.view">
        <field name="name">stock.in.line.form</field>
        <field name="model">nhom8.stock.in.line</field>
        <field name="arch" type="xml">
            <form string="Chi tiết phiếu nhập">
                <sheet>
                    <group>
                        <field name="stock_in_id" readonly="1"/>
                        <field name="product_id" required="1"/>
                        <field name="quantity" required="1"/>
                        <field name="purchase_price" required="1"/>
                        <field name="total_price" readonly="1"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_stock_in_line_tree" model="ir.ui.view">
        <field name="name">stock.in.line.tree</field>
        <field name="model">nhom8.stock.in.line</field>
        <field name="arch" type="xml">
            <list string="Chi tiết phiếu nhập">
                <field name="stock_in_id"/>
                <field name="product_id"/>
                <field name="quantity"/>
                <field name="purchase_price"/>
                <field name="total_price"/>
            </list>
        </field>
    </record>
</odoo>
